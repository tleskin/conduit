<request type="WirelessActivate">
  <originatingControlNumber><%= originating_control_number %></originatingControlNumber>
  <originatingClientId><%= originating_client_id %></originatingClientId>
  <originatingOrderId><%= originating_order_id %></originatingOrderId>
  <activity_type>NEW</activity_type>
  <planId><%= plan_id %></planId>
  <esn><%= esn %></esn>
  <physicalAddress>
    <firstName><%= first_name %></firstName>
    <lastName><%= last_name %></lastName>
    <address1><%= address1 %></address1>
    <address2><%= address2 %></address2>
    <city><%= city %></city>
    <state><%= state %></state>
    <zip><%= zip %></zip>
  </physicalAddress>
</request>
